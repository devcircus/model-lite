    "version": "1.0.0-beta.1",
    "keywords": [
        "bright-components",
        "laravel",
        "model",
        "eloquent"
    ],
    "homepage": "https://github.com/bright-components/model-lite",
    "license": "MIT",
    "authors": [
        {
            "name": "Clayton Stone",
            "email": "clay@phpstage.com",
            "homepage": "https://phpstage.com",
